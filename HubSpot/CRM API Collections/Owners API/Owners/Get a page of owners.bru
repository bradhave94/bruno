meta {
  name: Get a page of owners
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/crm/v3/owners/?email=<string>&after=<string>&limit=100&archived=false
  body: none
  auth: bearer
}

query {
  email: <string>
  after: <string>
  limit: 100
  archived: false
}

auth:bearer {
  token: {{LL_PA}}
}

headers {
  Accept: application/json
}
