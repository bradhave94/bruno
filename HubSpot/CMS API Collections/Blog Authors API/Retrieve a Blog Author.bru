meta {
  name: Retrieve a Blog Author
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/cms/v3/blogs/authors/:objectId?archived=<boolean>&property=<string>
  body: none
  auth: bearer
}

query {
  ~archived: <boolean>
  ~property: <string>
}

auth:bearer {
  token: {{LL_PA}}
}

headers {
  Accept: application/json
}
