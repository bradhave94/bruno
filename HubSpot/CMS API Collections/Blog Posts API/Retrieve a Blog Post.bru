meta {
  name: Retrieve a Blog Post
  type: http
  seq: 22
}

get {
  url: {{baseUrl}}/cms/v3/blogs/posts/:objectId?archived=<boolean>&property=<string>
  body: none
  auth: bearer
}

query {
  ~archived: <boolean>
  ~property: <string>
}

auth:bearer {
  token: {{LL_PA}}
}

headers {
  Accept: application/json
}
