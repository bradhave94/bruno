meta {
  name: Update a Folder
  type: http
  seq: 19
}

patch {
  url: {{baseUrl}}/cms/v3/pages/landing-pages/folders/:objectId?archived=<boolean>
  body: json
  auth: bearer
}

query {
  ~archived: <boolean>
}

auth:bearer {
  token: {{LL_PA}}
}

headers {
  Content-Type: application/json
  Accept: application/json
}

body:json {
  {
    "category": "<integer>",
    "created": "<dateTime>",
    "deletedAt": "<dateTime>",
    "id": "<string>",
    "name": "<string>",
    "parentFolderId": "<long>",
    "updated": "<dateTime>"
  }
}
